         <field name="key">veronica.url</field>
         <field name="value">https://api-sbox.veronica.ec/api/v1.0/oauth/token?grant_type=password</field>
      </record>

      <record id="config_parameter_veronica_base_url" model="ir.config_parameter" forcecreate="True">
         <field name="key">veronica.base.url</field>
         <field name="value">https://api-sbox.veronica.ec</field>
