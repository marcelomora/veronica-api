<?xml version="1.0" encoding="UTF-8"?>
<odoo>


      <record id="config_parameter_veronica_test_base_url" model="ir.config_parameter" forcecreate="True">
         <field name="key">veronica.base.test.url</field>
         <field name="value">https://api-sbox.veronica.ec</field>
      </record>

      <record id="config_parameter_veronica_prod_base_url" model="ir.config_parameter" forcecreate="True">
         <field name="key">veronica.base.prod.url</field>
         <field name="value">https://api.veronica.ec</field>
      </record>

      <record id="config_parameter_veronica_auth_url" model="ir.config_parameter" forcecreate="True">
         <field name="key">veronica.auth.url</field>
         <field name="value">/api/v1.0/oauth/token?grant_type=password</field>
      </record>

      <record id="config_parameter_veronica_comprobantes_url" model="ir.config_parameter" forcecreate="True">
         <field name="key">veronica.comprobantes.url</field>
         <field name="value">/api/v1.0/comprobantes</field>
      </record>

      <record id="config_parameter_veronica_sri_url" model="ir.config_parameter" forcecreate="True">
         <field name="key">veronica.sri.url</field>
         <field name="value">/api/v1.0/sri</field>
      </record>
         
</odoo>
